{"components/brand/show-more":"{{#if brand.facets}}\n  {{> components/faceted-search/show-more-facets brand}}\n{{/if}}\n","components/faceted-search/show-more-facets":"<ul class=\"facet-list-items faceted-search-option-columns\" data-facet-filter-wrapper>\n  {{#each facets}}\n    {{#if facet '===' ../all_facets}}\n      {{#each items}}\n        <li class=\"facet-item nav-list-item\" data-facet-item>\n          <a href=\"{{url}}\"\n          class=\"nav-list-action {{#if selected }}is-active{{/if}}\"\n          rel=\"nofollow\"\n          data-id=\"{{ id }}\"\n          data-faceted-search-facet>{{title}} {{#if ../show_product_counts}}<span data-facet-item-count>({{count}})</span>{{/if}}</a></li>\n      {{/each}}\n    {{/if}}\n  {{/each}}\n</ul>\n"}