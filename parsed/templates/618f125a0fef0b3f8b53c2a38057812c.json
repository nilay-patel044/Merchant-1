{"components/quick-results":"<div class=\"quick-search-inner\">\n  <div class=\"quick-search-results-text\">\n    {{lang 'search.quick_count' count=pagination.product_results.total search_query=forms.search.query}}\n  </div>\n  {{#if product_results.products}}\n    <ul class=\"quick-search-list\">\n      {{#each product_results.products}}\n        <li class=\"quick-search-item\">\n          <a class=\"quick-search-item-link\" href=\"{{url}}\">\n          <div class=\"quick-search-item-image\" style=\"background-image: url('{{getImage image 'thumb' (cdn ../theme_settings.default-product-image)}}');\">\n              <img class=\"sr-only\" src=\"{{getImage image 'thumb' (cdn ../theme_settings.default-product-image)}}\" alt=\"{{image.alt}}\">\n            </div>\n            <div class=\"quick-search-item-info\">\n              <div class=\"quick-search-item-title\" href=\"{{url}}\">{{name}}</div>\n              <div class=\"quick-search-item-brand\">{{brand.name}}</div>\n            </div>\n          </a>\n        </li>\n      {{/each}}\n    </ul>\n  {{/if}}\n  {{#if pagination.product_results.next}}\n    <div class=\"quick-search-button-wrapper\">\n      <a class=\"button button-primary\" href=\"{{forms.search.product_url}}\">\n        {{lang 'search.view_all'}}\n      </a>\n    </div>\n  {{/if}}\n</div>\n"}