{"components/faceted-search/facets/hierarchy":"<ul\n  class=\"facet-list-items {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\"\n  data-count=\"{{items_count}}\"\n  data-facet-filter-wrapper>\n\n  {{#each items}}\n    <li class=\"facet-item\" data-facet-item>\n      <a href=\"{{url}}\"\n      rel=\"nofollow\"\n      data-faceted-search-facet>\n        {{title}}\n        {{#if ../show_product_counts}}<span class=\"facet-item-count\" data-facet-item-count>({{count}})</span>{{/if}}\n      </a>\n    </li>\n  {{/each}}\n</ul>\n\n{{#if show_more_toggle}}\n  <a class=\"facet-toggle-more\" href=\"#\" data-facet-more=\"{{facet}}\">\n    <span class=\"facet-item toggle-link-text-off\">\n      {{lang 'faceted_search.show_more'}}\n      <svg class=\"icon icon-arrow-down\"><use xlink:href=\"#icon-arrow-down\" /></svg>\n    </span>\n    <span class=\"facet-item toggle-link-text-on\">\n      {{lang 'faceted_search.show_less'}}\n      <svg class=\"icon icon-arrow-up\"><use xlink:href=\"#icon-arrow-up\" /></svg>\n    </span>\n  </a>\n{{/if}}\n"}