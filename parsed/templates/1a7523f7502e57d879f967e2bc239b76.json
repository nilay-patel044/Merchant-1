{"components/products/product-single-tabs":"<section class=\"product-tabs\" data-product-container>\n  <ul class=\"tabs\" data-tabs>\n    <li class=\"tab-title\"><a href=\"#product-description\">{{lang 'product.description'}}</a></li>\n    {{#if settings.show_product_reviews}}\n      <li class=\"tab-title\">\n        <a href=\"#reviews\">\n          {{lang 'product.reviews_tab_title'}}\n          <span class=\"num-reviews\" data-num-reviews=\"({{product.reviews.list.length}})\"></span>\n        </a>\n      </li>\n    {{/if}}\n    <li class=\"tab-title\"><a href=\"#product-additional-info\">{{lang 'product.additional_info'}}</a></li>\n    {{#if product.videos.list.length}}\n      <li class=\"tab-title {{#if theme_settings.product-single-videos-count '==' 0}}video-disabled{{/if}}\"><a href=\"#product-videos\">{{lang 'product.video'}}</a></li>\n    {{/if}}\n  </ul>\n\n  <div class=\"tabs-content\">\n    <span class=\"accordion-title\">\n      <a href=\"#product-description\" data-scroll=\"#product-description\">\n        {{lang 'product.description'}}\n        <svg class=\"icon icon-chevron-down\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n      </a>\n    </span>\n    <div class=\"tab-content-panel product-description\" id=\"product-description\">\n      <h2>{{lang 'product.description'}}</h2>\n      <div class=\"product-description-wrapper\">\n        {{{product.description}}}\n        {{{snippet 'product_description'}}}\n      </div>\n    </div>\n\n    {{#if settings.show_product_reviews}}\n      <span class=\"accordion-title\">\n        <a href=\"#reviews\" data-scroll=\"#reviews\">\n          {{lang 'product.reviews_tab_title'}}\n          <svg class=\"icon icon-chevron-down\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n        </a>\n      </span>\n      <div class=\"tab-content-panel\" id=\"reviews\">\n        <div class=\"product-reviews-header\">\n          <h2 class=\"product-reviews-title\">\n            {{lang 'product.reviews.title' numReviews=product.num_reviews}}\n          </h2>\n          <button class=\"button-review-modal button\">{{lang 'product.reviews.write_review'}}</button>\n        </div>\n\n        {{{snippet 'reviews'}}}\n\n        {{#if product.reviews.list.length}}\n          <div class=\"review-items\">\n            {{#each product.reviews.list}}\n              {{> components/products/review-item index=@index}}\n            {{/each}}\n          </div>\n\n          {{#if product.reviews.list.length '>' 2}}\n            <div class=\"reviews-more-wrap\">\n              <a class=\"reviews-show-more-link\" href=\"#\">\n                {{lang 'product.reviews.show_more'}}\n                <svg class=\"icon icon-arrow-down\"><use xlink:href=\"#icon-arrow-down\" /></svg>\n              </a>\n            </div>\n          {{/if}}\n        {{else}}\n          <h5>{{lang 'product.reviews.no_reviews'}}</h5>\n        {{/if}}\n\n        <div class=\"modal-content-wrap\" id=\"review-modal\">\n          {{> components/products/review-form}}\n        </div>\n      </div>\n    {{/if}}\n\n    <span class=\"accordion-title\">\n      <a href=\"#product-additional-info\" data-scroll=\"#product-additional-info\">\n        {{lang 'product.additional_info'}}\n        <svg class=\"icon icon-chevron-down\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n      </a>\n    </span>\n    <div class=\"tab-content-panel\" id=\"product-additional-info\">\n      <h2>{{lang 'product.additional_info'}}</h2>\n\n      {{#if product.warranty}}\n        <div class=\"product-warranty\">\n          <dl class=\"product-details-block\">\n            <dt class=\"product-details-name\">{{lang 'product.warranty'}}:</dt>\n            <dd class=\"product-details-value\">{{product.warranty}}</dd>\n          </dl>\n        </div>\n      {{/if}}\n\n      <div class=\"product-details-blocks\">\n        {{#if product.sku}}\n          <dl class=\"product-details-block product-details-sku\">\n            <dt class=\"product-details-name\">{{lang 'product.sku'}}:</dt>\n            <dd class=\"product-details-value\" data-product-sku>{{product.sku}}</dd>\n          </dl>\n        {{/if}}\n        {{#if product.condition}}\n          <dl class=\"product-details-block product-details-condition\">\n            <dt class=\"product-details-name\">{{lang 'product.condition'}}:</dt>\n            <dd class=\"product-details-value\">{{product.condition}}</dd>\n          </dl>\n        {{/if}}\n        {{#if product.availability}}\n          <dl class=\"product-details-block product-details-availability\">\n            <dt class=\"product-details-name\">{{lang 'product.availability'}}:</dt>\n            <dd class=\"product-details-value\">{{product.availability}}</dd>\n          </dl>\n        {{/if}}\n        {{#if product.weight}}\n          <dl class=\"product-details-block product-details-weight\">\n            <dt class=\"product-details-name\">{{lang 'product.weight'}}:</dt>\n            <dd class=\"product-details-value\" data-product-weight>{{product.weight}}</dd>\n          </dl>\n        {{/if}}\n\n        {{#all product.width theme_settings.show-product-dimensions}}\n          <dl class=\"product-details-block product-details-width\">\n            <dt class=\"product-details-name\">{{lang 'product.width'}}</dt>\n            <dd class=\"product-details-value\">{{product.width}}</dd>\n          </dl>\n        {{/all}}\n        {{#all product.height theme_settings.show-product-dimensions}}\n          <dl class=\"product-details-block product-details-height\">\n            <dt class=\"product-details-name\">{{lang 'product.height'}}</dt>\n            <dd class=\"product-details-value\">{{product.height}}</dd>\n          </dl>\n        {{/all}}\n        {{#all product.depth theme_settings.show-product-dimensions}}\n          <dl class=\"product-details-block product-details-depth\">\n            <dt class=\"product-details-name\">{{lang 'product.depth'}}</dt>\n            <dd class=\"product-details-value\">{{product.depth}}</dd>\n          </dl>\n        {{/all}}\n\n        {{#if product.gift_wrapping_available}}\n          <dl class=\"product-details-block product-details-giftwrapping\">\n            <dt class=\"product-details-name\">{{lang 'product.gift_wrapping'}}:</dt>\n            <dd class=\"product-details-value\">{{lang 'product.gift_wrapping_available'}}</dd>\n          </dl>\n        {{/if}}\n        {{#if product.shipping}}\n          <dl class=\"product-details-block product-details-shippping\">\n            {{#if product.shipping.calculated}}\n              <dt class=\"product-details-name\">{{lang 'product.shipping'}}:</dt>\n              <dd class=\"product-details-value\">{{lang 'product.shipping_calculated'}}</dd>\n            {{else}}\n              {{#if product.shipping.price '===' 0}}\n                <dt class=\"product-details-name\">{{lang 'product.shipping'}}:</dt>\n                <dd class=\"product-details-value\">{{lang 'product.shipping_free'}}</dd>\n              {{else}}\n                <dt class=\"product-details-name\">{{lang 'product.shipping'}}:</dt>\n                <dd class=\"product-details-value\">{{lang 'product.shipping_fixed' amount=product.shipping.price.formatted}}</dd>\n              {{/if}}\n            {{/if}}\n          </dl>\n        {{/if}}\n\n        {{#each product.custom_fields}}\n          <dl class=\"product-details-block product-details-custom\">\n            <dt class=\"product-details-name\">{{name}}:</dt>\n            <dd class=\"product-details-value\">{{value}}</dd>\n          </dl>\n        {{/each}}\n      </div>\n    </div>\n\n    {{#if product.videos.list.length}}\n      <span class=\"accordion-title\">\n        <a href=\"#product-videos\" data-scroll=\"#product-videos\">\n          {{lang 'product.video'}}\n          <svg class=\"icon icon-chevron-down\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n        </a>\n      </span>\n      <div class=\"tab-content-panel {{#if theme_settings.product-single-videos-count '==' 0}}video-disabled{{/if}}\" id=\"product-videos\">\n        <div class=\"product-videos-list\">\n          {{#each product.videos.list}}\n            {{> components/products/video-block video=this}}\n          {{/each}}\n        </div>\n      </div>\n    {{/if}}\n\n    <svg class=\"icon icon-spinner\"><use xlink:href=\"#icon-spinner\" /></svg>\n  </div>\n\n  {{#if settings.add_this.buttons}}\n    <div class=\"share-block {{#any settings.add_this.buttons service='facebook_like'}}has-like{{/any}}\">\n      <span class=\"form-label-text share-title\">{{lang 'product.share'}}</span>\n      {{> components/common/share-buttons share_buttons=settings.add_this url=product.url}}\n    </div>\n  {{/if}}\n\n</section>\n","components/products/review-item":"<article class=\"review-item {{#if index '>' 1}}hidden{{/if}}\">\n  <div class=\"review-meta\">\n    <div class=\"product-item-rating\">\n      {{> components/products/ratings rating=rating size=\"medium\"}}\n    </div>\n    {{#if name.length}}\n      <span class=\"review-author\">{{name}}</span>\n    {{/if}}\n    <span class=\"review-date\">{{date}}</span>\n  </div>\n\n  <h3 class=\"review-item-title\">{{title}}</h3>\n  <p class=\"review-item-body\">{{text}}</p>\n</article>\n","components/products/review-form":"<div class=\"review-form\">\n  {{> components/common/modal-close}}\n  <h2 class=\"review-form-heading\">{{lang 'product.reviews.write_review'}}</h2>\n  <form class=\"form\" id=\"form-leave-a-review\" action=\"{{urls.product.post_review}}\" method=\"post\" data-validated-form>\n    <!-- Name -->\n    <div class=\"form-field rating-name\">\n      <label class=\"form-label\" for=\"rating-name\">\n        <div class=\"form-label-text\">{{lang 'product.reviews.form.name'}}:</div>\n        <input id=\"rating-name\" class=\"form-input\" type=\"text\" placeholder=\"{{lang 'product.reviews.form.your_name'}}\" name=\"revfromname\" required>\n      </label>\n    </div>\n\n    <!-- Email -->\n    <div class=\"form-field rating-email\">\n      <label class=\"form-label\" for=\"rating-email\">\n        <div class=\"form-label-text\">{{lang 'product.reviews.form.email'}}:</div>\n        <input id=\"rating-email\" class=\"form-input\" type=\"email\" name=\"email\" placeholder=\"{{lang 'product.reviews.form.your_email'}}\" required>\n      </label>\n    </div>\n\n    <!-- Rating -->\n    <div class=\"form-field rating-number\">\n      <label class=\"form-label\" for=\"rating-stars\">\n        <div class=\"form-label-text\">{{lang 'product.reviews.form.rating_label'}}:</div>\n        <div class=\"rating-stars-wrap\">\n          <select class=\"form-input input-select rating-stars\" id=\"rating-stars\" name=\"revrating\">\n            {{#for 1 5}}\n              <option value=\"{{$index}}\" {{#if $index '===' 5}}selected{{/if}}>{{lang 'product.reviews.stars_count' NUM=$index}}</option>\n            {{/for}}\n          </select>\n          <div class=\"product-item-rating\">\n            <span class=\"rating-stars-label\">{{lang 'product.reviews.stars_count' NUM=5}}</span>\n            {{#for 1 5}}\n              <span class=\"icon-star-wrap full\">\n                <svg class=\"icon icon-star\"><use xlink:href=\"#icon-star\" /></svg>\n              </span>\n            {{/for}}\n            <svg class=\"icon icon-arrow-down\"><use xlink:href=\"#icon-arrow-down\" /></svg>\n          </div>\n        </div>\n      </label>\n    </div>\n\n    <!-- Review Subject -->\n    <div class=\"form-field rating-subject\">\n      <label class=\"form-label\" for=\"rating-title\">\n        <div class=\"form-label-text\">{{lang 'product.reviews.form.subject'}}:</div>\n        <input id=\"rating-title\" class=\"form-input\" type=\"text\" placeholder=\"{{lang 'product.reviews.form.your_subject'}}\" name=\"revtitle\" required>\n      </label>\n    </div>\n\n    <!-- Comments -->\n    <div class=\"form-field\">\n      <label class=\"form-label\" for=\"rating-comment\">\n        <div class=\"form-label-text\">{{lang 'product.reviews.form.comments'}}:</div>\n        <textarea id=\"rating-comment\" class=\"form-input\" name=\"revtext\" placeholder=\"{{lang 'product.reviews.form.your_comments'}}\" rows=\"4\" required></textarea>\n      </label>\n    </div>\n\n    {{#if product.reviews.captcha}}\n      {{> core/forms/common/captcha captcha=product.reviews.captcha name=recaptcha_response_field}}\n    {{/if}}\n\n    <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" />\n    <input type=\"hidden\" name=\"action\" value=\"post_review\" />\n    {{#if product.reviews.throttleToken}}\n      <input type=\"hidden\" name=\"throttleToken\" value=\"{{product.reviews.throttleToken}}\">\n    {{/if}}\n\n    <div class=\"form-submit-wrap\">\n      <input type=\"submit\" class=\"form-submit button button-primary button-wide\" value=\"{{lang 'product.reviews.form.submit'}}\">\n      <button class=\"button button-secondary\">{{lang 'product.reviews.form.cancel'}}</button>\n    </div>\n  </form>\n</div>\n","components/products/video-block":"<div class=\"video-container\">\n  <iframe src=\"https://www.youtube.com/embed/{{id}}?rel=0&showinfo=0&autohide=1\" frameborder=\"0\" allowfullscreen></iframe>\n</div>\n","components/common/share-buttons":"{{#if share_buttons}}\n  <nav class=\"share-buttons-nav\">\n    <ul class=\"share-buttons\" data-share-buttons>\n\n      {{#each share_buttons.buttons}}\n        {{#if service '===' 'facebook_like'}}\n          <li class=\"social-link facebook-like\">\n            <div class=\"fb-like\"\n              data-href=\"{{../../url}}\"\n              data-layout=\"button_count\"\n              data-action=\"like\"\n              data-show-faces=\"false\">\n            </div>\n          </li>\n        {{/if}}\n      {{/each}}\n\n      {{#each share_buttons.buttons}}\n        {{#if service '===' 'facebook'}}\n          <li class=\"social-link facebook\">\n            <a target=\"_blank\" href=\"http://www.facebook.com/sharer.php?u={{../../url}}\" class=\"facebook\">\n              {{> svg/social/facebook}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'twitter'}}\n          <li class=\"social-link twitter\">\n            <a target=\"_blank\" href=\"https://twitter.com/share?url={{../../url}}&amp;text={{../../title}}\" class=\"twitter\">\n              {{> svg/social/twitter}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'google_plusone'}}\n          <li class=\"social-link google-plus\">\n            <a target=\"_blank\" href=\"https://plus.google.com/share?url={{../../url}}\" class=\"google\">\n              {{> svg/social/googleplus}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'linkedin'}}\n          <li class=\"social-link linkedin\">\n            <a target=\"_blank\" href=\"http://www.linkedin.com/shareArticle?url={{../../url}}&amp;title={{../../title}}\" class=\"linkedin\">\n              {{> svg/social/linkedin}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'email'}}\n          <li class=\"social-link mail\">\n            <a href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\" class=\"email\">\n              {{> svg/social/email}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'print'}}\n          <li class=\"social-link print\">\n            <a class=\"print\" href=\"javascript:;\" onclick=\"window.print()\">\n              {{> svg/social/print}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'pinterest'}}\n          <li class=\"social-link pinterest\">\n            <a target=\"_blank\" href=\"//pinterest.com/pin/create/button/?url={{../../url}}&amp;media={{../../image}}&amp;description={{../../title}}\" class=\"share-pinterest\">\n              {{> svg/social/pinterest}}\n            </a>\n          </li>\n        {{/if}}\n      {{/each}}\n\n    </ul>\n  </nav>\n{{/if}}\n","components/products/ratings":"{{#for 1 5}}\n  <svg class=\"icon icon-star {{#if ../this.rating '>=' $index}}full{{/if}}\"><use xlink:href=\"#icon-star\" /></svg>\n{{/for}}\n","components/common/modal-close":"<a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'cart.modal_close'}}\">\n  <span aria-hidden=\"true\">\n    <svg class=\"icon icon-close\"><use xlink:href=\"#icon-close\" /></svg>\n  </span>\n</a>\n","core/forms/common/captcha":"{{!\n  Basically just a wrapper.\n  If you call this file it should serve the correct markup based on where it's called from.\n}}\n\n{{#if product.reviews.recaptcha}}\n  {{{product.reviews.recaptcha.markup}}}\n{{/if}}\n{{#if forms.create_account.recaptcha}}\n  {{{forms.create_account.recaptcha.markup}}}\n{{/if}}\n{{#if forms.contact.recaptcha}}\n  {{{forms.contact.recaptcha.markup}}}\n{{/if}}\n","svg/social/facebook":"<svg width=\"8\" height=\"16\" viewBox=\"0 0 8 16\" xmlns=\"http://www.w3.org/2000/svg\"><title>facebook</title><path d=\"M0 5.292h1.655V3.684c0-.71.018-1.803.532-2.48C2.73.487 3.474 0 4.755 0 6.842 0 7.72.297 7.72.297l-.412 2.45s-.69-.198-1.333-.198c-.644 0-1.22.23-1.22.872v1.87h2.638L7.21 7.685H4.754V16h-3.1V7.685H0V5.292\"/></svg>\n","svg/social/twitter":"<svg width=\"18\" height=\"14\" viewBox=\"0 0 18 14\" xmlns=\"http://www.w3.org/2000/svg\"><title>twitter</title><path d=\"M17.228 1.657c-.633.28-1.315.472-2.03.557.73-.437 1.29-1.13 1.554-1.955-.683.403-1.438.698-2.245.855C13.863.43 12.944 0 11.927 0c-1.95 0-3.533 1.583-3.533 3.534 0 .277.03.546.09.805C5.548 4.19 2.945 2.785 1.2.644.894 1.17.72 1.777.72 2.425c0 1.224.625 2.306 1.573 2.94-.578-.017-1.124-.178-1.6-.44v.043c0 1.713 1.217 3.142 2.835 3.465-.296.083-.61.125-.93.125-.23 0-.45-.02-.667-.063.45 1.404 1.756 2.426 3.303 2.453-1.21.95-2.734 1.514-4.39 1.514-.285 0-.567-.016-.843-.05C1.564 13.416 3.423 14 5.418 14c6.502 0 10.056-5.385 10.056-10.055 0-.154-.003-.308-.01-.458.692-.5 1.292-1.12 1.764-1.83\"/></svg>\n","svg/social/googleplus":"<svg width=\"21\" height=\"14\" viewBox=\"0 0 21 14\" xmlns=\"http://www.w3.org/2000/svg\"><title>google</title><path d=\"M6.668 5.523v2.79h3.595c-.573 1.804-1.46 2.79-3.595 2.79-2.188 0-3.907-1.86-3.907-4.158 0-2.297 1.72-4.156 3.908-4.156 1.146 0 1.928.437 2.605 1.038.573-.6.52-.656 1.875-2.023C9.95.655 8.388 0 6.668 0 2.968 0 0 3.117 0 7s2.97 7 6.668 7c5.522 0 6.877-5.03 6.408-8.422H6.668v-.055zm11.93.22V3.28h-1.615v2.462h-2.397v1.696h2.397v2.515h1.615V7.438h2.344V5.742h-2.344z\"/></svg>\n","svg/social/linkedin":"<svg width=\"18\" height=\"16\" viewBox=\"0 0 18 16\" xmlns=\"http://www.w3.org/2000/svg\"><title>linkedin</title><path d=\"M17.425 16h-3.85v-5.625c0-1.472-.603-2.477-1.928-2.477-1.013 0-1.577.678-1.84 1.332-.098.235-.082.56-.082.888V16H5.91s.05-9.964 0-10.87h3.815v1.707c.225-.746 1.444-1.81 3.39-1.81 2.414 0 4.31 1.563 4.31 4.93V16zM2.05 3.77h-.023C.797 3.77 0 2.94 0 1.888 0 .813.82 0 2.075 0c1.253 0 2.023.81 2.048 1.884 0 1.053-.795 1.887-2.072 1.887zM.44 5.13h3.396V16H.44V5.13z\"/></svg>\n","svg/social/email":"<svg width=\"19\" height=\"14\" viewBox=\"0 0 19 14\" xmlns=\"http://www.w3.org/2000/svg\"><title>email</title><path d=\"M17.5 0H1.167C.52 0 0 .52 0 1.167v11.666C0 13.48.52 14 1.167 14H17.5c.645 0 1.167-.52 1.167-1.167V1.167C18.667.52 18.145 0 17.5 0zm-1.167 2.333v.077l-7 5.445-7-5.445v-.077h14zm-14 9.334v-6.3l6.284 4.887c.21.164.463.246.716.246.253 0 .507-.082.717-.246l6.283-4.887v6.3h-14z\"/></svg>\n","svg/social/print":"<svg width=\"19\" height=\"16\" viewBox=\"0 0 19 16\" xmlns=\"http://www.w3.org/2000/svg\"><title>printer</title><path d=\"M17.776 3.62h-3.09V.53c0-.293-.24-.53-.532-.53H4.152c-.292 0-.53.237-.53.53v3.092H.53c-.293 0-.53.236-.53.53v8.277c0 .292.237.53.53.53h2.955v2.51c0 .293.237.53.53.53h10.14c.292 0 .53-.237.53-.53v-2.51h3.09c.294 0 .53-.238.53-.53V4.15c.002-.292-.235-.53-.53-.53zm-12.404 0V1.75h7.563v1.87H5.372zm7.574 4.323v6.307h-7.6V7.943h7.6z\"/></svg>\n","svg/social/pinterest":"<svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" xmlns=\"http://www.w3.org/2000/svg\"><title>pinterest</title><path d=\"M8 0C3.583 0 0 3.582 0 8c0 3.275 1.97 6.09 4.79 7.327-.023-.558-.004-1.23.138-1.837l1.03-4.36S5.7 8.62 5.7 7.866c0-1.185.688-2.07 1.543-2.07.727 0 1.08.546 1.08 1.2 0 .732-.468 1.826-.707 2.84-.2.85.425 1.54 1.262 1.54 1.516 0 2.536-1.946 2.536-4.253 0-1.753-1.18-3.065-3.328-3.065-2.426 0-3.938 1.81-3.938 3.83 0 .698.205 1.19.527 1.57.148.175.168.244.114.446l-.16.64c-.055.204-.22.277-.4.202-1.12-.457-1.64-1.68-1.64-3.057 0-2.272 1.916-4.998 5.717-4.998 3.055 0 5.065 2.212 5.065 4.583 0 3.14-1.744 5.483-4.317 5.483-.863 0-1.675-.466-1.954-.996 0 0-.464 1.843-.562 2.198-.17.617-.5 1.234-.805 1.714.72.212 1.48.328 2.267.328 4.418 0 8-3.582 8-8s-3.582-8-8-8\"/></svg>\n"}