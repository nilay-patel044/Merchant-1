{"components/carousel":"<section\n  class=\"carousel {{#if carousel.slides.length '===' 1}}single-slide{{/if}} {{theme_settings.home-carousel-aspect-ratio}}\"\n  data-swap-frequency=\"{{carousel.swap_frequency}}\">\n  {{#each carousel.slides}}\n    <figure class=\"carousel-item {{#if @first}}active{{/if}}\">\n      {{#unless button_text}}\n        {{#if url}}<a href=\"{{url}}\">{{/if}}\n      {{/unless}}\n      <div class=\"carousel-item-image\" style=\"background-image: url('{{image}}');\">\n        <img class=\"home-carousel-image\" src=\"{{image}}\" alt=\"{{{alt_text}}}\" />\n      </div>\n      {{#unless button_text}}\n        {{#if url}}</a>{{/if}}\n      {{/unless}}\n      <figcaption class=\"carousel-item-info\">\n        {{#if heading}}<h2 class=\"carousel-item-title caption-content{{#if ../../theme_settings.home-carousel-show-slide-number}} show-slide-number{{/if}}\">{{{heading}}}</h2>{{/if}}\n        {{#if text}}<p class=\"carousel-item-caption caption-content\">{{{text}}}</p>{{/if}}\n        {{#if button_text}}\n          <a class=\"button button-primary caption-content\" href=\"{{url}}\">{{{button_text}}}</a>\n        {{/if}}\n        <span class=\"carousel-navigation-item previous\">\n          {{> svg/arrow-left}}\n        </span>\n        <span class=\"carousel-navigation-item next\">\n          {{> svg/arrow-right}}\n        </span>\n      </figcaption>\n    </figure>\n  {{/each}}\n</section>\n","svg/arrow-left":"<svg width=\"13\" height=\"22\" viewBox=\"0 0 13 22\" xmlns=\"http://www.w3.org/2000/svg\"><title>Arrow left</title><path d=\"M10.014 21.064L.938 12.05c-.584-.58-.584-1.52 0-2.1L10.014.937C10.3.653 10.68.5 11.074.5c.397 0 .777.153 1.063.436.583.58.583 1.52 0 2.1l-7.733 7.608-.362.356.362.356 7.734 7.61c.582.58.582 1.518-.002 2.098-.585.58-1.536.58-2.122 0z\" /></svg>\n","svg/arrow-right":"<svg width=\"13\" height=\"22\" viewBox=\"0 0 13 22\" xmlns=\"http://www.w3.org/2000/svg\"><title>Arrow right</title><path d=\"M12.136 9.95c.585.58.585 1.5