{"components/quick-shop/quick-shop-modal":"{{> components/common/modal-close}}\n\n<div class=\"modal-body\">\n  {{> components/products/product-single-details quick_view=true}}\n</div>\n","components/common/modal-close":"<a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'cart.modal_close'}}\">\n  <span aria-hidden=\"true\">\n    <svg class=\"icon icon-close\"><use xlink:href=\"#icon-close\" /></svg>\n  </span>\n</a>\n","components/products/product-single-details":"<section class=\"product-details product-block section {{#unless quick_view}}product-single{{/unless}}\" data-product-container data-product-title=\"{{product.title}}\" data-product-id=\"{{product.id}}\">\n\n  <div class=\"container\">\n    <div class=\"product-details-column {{#if product.images}}has-images{{/if}}\">\n      {{> components/products/product-single-images images=product.images}}\n    </div>\n\n    <div class=\"product-details-column {{#if product.images}}has-images{{/if}}\">\n      {{#if product.brand}}\n        <a class=\"product-brand\" href=\"{{product.brand.url}}\">{{product.brand.name}}</a>\n      {{/if}}\n\n      {{#if quick_view}}\n        <h1 class=\"product-title\"><a href=\"{{product.url}}\">{{this.product.title}}</a></h1>\n      {{else}}\n        <h1 class=\"product-title\">{{this.product.title}}</h1>\n      {{/if}}\n\n      <div class=\"product-item-price\">\n        {{#if product.price}}\n          {{> components/products/price\n            price=product.price\n            show_savings=true\n            save_block=true\n            schema_org=true\n          }}\n        {{/if}}\n\n        {{#if product.call_for_price}}\n          <p>{{product.call_for_price}}</p>\n        {{/if}}\n      </div>\n\n      <div class=\"product-rating-block\">\n        {{#if settings.show_product_rating}}\n          <div class=\"product-item-rating\">\n            {{> components/products/ratings rating=this.product.rating}}\n            <span class=\"total-reviews reviews-jumplink\" data-scroll=\"#reviews\">\n              <a class=\"product-reviews-link\" href=\"{{#if quick_view}}{{product.url}}{{/if}}#reviews\">{{lang 'product.reviews.title' numReviews=product.num_reviews}}</a>\n            </span>\n          </div>\n\n          {{{snippet 'product_rating'}}}\n        {{/if}}\n      </div>\n\n      {{#if product.release_date }}\n        <p class=\"message neutral\">{{product.release_date}}</p>\n      {{/if}}\n\n      <form method=\"post\" id=\"product-add-cart\" action=\"{{product.cart_url}}\" enctype=\"multipart/form-data\" data-cart-item-add data-product-options-count=\"{{product.options.length}}\">\n        <input type=\"hidden\" name=\"action\" value=\"add\">\n        <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" data-product-id/>\n\n        {{#if product.customizations.length}}\n          <div class=\"product-options-container\">\n            {{#each product.customizations}}\n              {{{dynamicComponent 'core/forms/customizations'}}}\n            {{/each}}\n          </div>\n        {{/if}}\n\n        {{#if product.options.length}}\n          <div class=\"product-options-container\" data-product-option-change>\n            {{#each product.options}}\n              {{{dynamicComponent 'core/forms/options'}}}\n            {{/each}}\n          </div>\n        {{/if}}\n\n        {{#if product.event_date}}\n          {{> core/products/event}}\n        {{/if}}\n\n        {{> components/products/bulk-pricing}}\n\n        <div class=\"product-stock {{#unless product.show_quantity_input}}default-layout{{/unless}}\">\n          <span class=\"{{#unless product.stock_level}}product-details-hidden{{/unless}}\" data-product-stock>{{lang 'product.current_stock'}}</span>\n          <span class=\"{{#unless product.stock_level}}product-details-hidden{{/unless}}\" data-product-stock data-product-stock-level>{{#if product.stock_level}}{{product.stock_level}}{{else}}0{{/if}}</span>\n\n          {{#if product.out_of_stock}}\n            <span class=\"stock-message\">\n            {{#if product.out_of_stock_message}}\n              {{product.out_of_stock_message}}\n            {{else}}\n              {{lang 'product.sold_out'}}\n            {{/if}}\n            <span/>\n          {{/if}}\n        </div>\n\n        {{#if product.show_quantity_input}}\n          <div class=\"product-quantity-container\">\n            <div class=\"form-field\">\n              <label class=\"form-label\">\n                <span class=\"form-label-text product-option-title\">{{ lang 'product.quantity'}}</span>\n                {{> components/common/quantity product}}\n              </label>\n            </div>\n          </div>\n        {{/if}}\n\n        {{#if product.can_purchase}}\n          <button\n            type=\"submit\"\n            class=\"button button-primary button-wide add-to-cart button-progress spinner\"\n            data-progress-complete=\"{{lang 'product.added'}}\"\n            data-button-purchase\n          >\n            <span class=\"button-text\">\n              {{#if product.pre_order}}\n                {{lang 'product.pre_order'}}\n              {{else}}\n                {{lang 'product.add_to_cart'}}\n              {{/if}}\n            </span>\n          </button>\n        {{else}}\n          {{#if product.out_of_stock}}\n            <div class=\"button button-primary button-wide add-to-cart button-disabled\">\n              {{lang 'product.sold_out'}}\n            </div>\n          {{/if}}\n        {{/if}}\n      </form>\n\n      <div class=\"product-actions\">\n        {{#if settings.show_wishlist}}\n          <form method=\"post\" class=\"wishlist-form\" action=\"{{product.add_to_wishlist_url}}\" {{#if customer}}data-customer{{/if}}>\n            <button type=\"submit\" class=\"button button-secondary button-wide add-to-wishlist button-progress spinner\">\n              <span class=\"button-text\">{{lang 'product.add_to_wishlist'}}</span>\n            </button>\n          </form>\n        {{/if}}\n\n        <div class=\"product-message-area\" data-product-message></div>\n\n        {{#if quick_view}}\n          <a class=\"quick-shop-details-lower link\" href=\"{{this.product.url}}\">{{lang 'product.view_full_details'}}</a>\n        {{/if}}\n      </div>\n\n      {{#any settings.add_this.buttons service='facebook_like'}}\n        <div class=\"share-block\">\n          <span class=\"form-label-text share-title\">{{lang 'product.share'}}</span>\n          {{> components/common/share-buttons share_buttons=settings.add_this url=product.url image=(getImage product.main_image 'thumb') title=product.title}}\n        </div>\n      {{/any}}\n\n      {{{snippet 'product_details'}}}\n    </div>\n  </div>\n</section>\n","components/products/product-single-images":"<div class=\"product-images-container\">\n  <div class=\"product-slides-container\">\n    <div class=\"product-slides-wrap\" data-adaptive-height=\"{{theme_settings.product-single-image-adaptive-height}}\">\n      {{#each images}}\n        <a class=\"product-image\" href=\"{{getImage this 'zoom'}}\" data-product-image>\n          <img src=\"{{getImage this 'product'}}\" alt=\"{{alt}}\">\n        </a>\n      {{/each}}\n    </div>\n  </div>\n  <ul class=\"product-images-pagination {{#if images.length '==' 1}}hidden{{/if}}\">\n    {{#each images}}\n      <li class=\"pagination-item\">\n        <a href=\"#\" data-slide-to=\"{{@index}}\" style=\"background-image: url('{{getImage this 'thumb'}}');\">\n          <img class=\"sr-only\" src=\"{{getImage this 'thumb'}}\" alt=\"{{this.alt}}\">\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n</div>\n","components/products/price":"<div class=\"{{#if price.saved}}price-saved{{/if}}\">\n  {{#if price}}\n    {{#or has_options product.options.length}}\n      {{#if price.with_tax}}\n        <div\n          class=\"product-price-line{{#if price.without_tax}} price-second-line{{/if}}\"\n          data-product-price-wrapper=\"with-tax\">\n\n          {{#if page_type '!==' 'product'}}\n            {{#if as_low_as}}\n              <span class=\"as-low-as-pricing\">{{lang 'product.as_low_as_price_message'}}</span>\n            {{/if}}\n          {{/if}}\n\n          {{#if price.saved}}\n            <span class=\"price-rrp\">{{price.rrp_with_tax.formatted}}</span>\n          {{/if}}\n\n          <span class=\"price-value\"> {{price.with_tax.formatted}}</span>\n\n          {{#if price.without_tax}}\n            <span class=\"tax-label\"> {{lang 'product.including_tax'}}</span>\n          {{/if}}\n        </div>\n      {{/if}}\n\n      {{#if price.without_tax}}\n        <div class=\"product-price-line\" data-product-price-wrapper=\"without-tax\">\n\n          {{#if page_type '!==' 'product'}}\n            {{#if as_low_as}}\n              <span class=\"as-low-as-pricing\">{{lang 'product.as_low_as_price_message'}}</span>\n            {{/if}}\n          {{/if}}\n\n          {{#if price.saved}}\n            <span class=\"price-rrp\">{{price.rrp_without_tax.formatted}}</span>\n          {{/if}}\n\n          <span class=\"price-value\"> {{price.without_tax.formatted}}</span>\n\n          {{#if price.with_tax}}\n            <span class=\"tax-label\"> {{lang 'product.excluding_tax'}}</span>\n          {{/if}}\n        </div>\n      {{/if}}\n\n      {{#if show_savings}}\n        {{#if price.saved}}\n          <div class=\"price-you-save\" data-product-price-saved>\n            {{lang 'product.you_save'}} {{price.saved.formatted}}\n          </div>\n        {{/if}}\n      {{/if}}\n    {{else}}\n      {{#if price.without_tax}}\n        <div class=\"product-price-line\" data-product-price-wrapper=\"without-tax\">\n\n          {{#if price.saved}}\n            <span class=\"price-rrp\">{{price.rrp_without_tax.formatted}}</span>\n          {{/if}}\n\n          <span class=\"price-value\"> {{price.without_tax.formatted}}</span>\n\n          {{#if price.with_tax}}\n            <span class=\"tax-label\"> {{lang 'product.excluding_tax'}}</span>\n          {{/if}}\n        </div>\n      {{/if}}\n\n      {{#if price.with_tax}}\n        <div class=\"product-price-line{{#if price.without_tax}} price-second-line{{/if}}\" data-product-price-wrapper=\"with-tax\">\n\n          {{#if price.saved}}\n            <span class=\"price-rrp\">{{price.rrp_with_tax.formatted}}</span>\n          {{/if}}\n\n          <span class=\"price-value\">{{price.with_tax.formatted}}</span>\n\n          {{#if price.without_tax}}\n            <span class=\"tax-label\"> {{lang 'product.including_tax'}}</span>\n          {{/if}}\n        </div>\n      {{/if}}\n\n      {{#if show_savings}}\n        {{#if price.saved}}\n          <div class=\"price-you-save\" data-product-price-saved>\n            {{lang 'product.you_save'}} {{price.saved.formatted}}\n          </div>\n        {{/if}}\n      {{/if}}\n    {{/or}}\n  {{/if}}\n</div>\n","components/products/ratings":"{{#for 1 5}}\n  <svg class=\"icon icon-star {{#if ../this.rating '>=' $index}}full{{/if}}\"><use xlink:href=\"#icon-star\" /></svg>\n{{/for}}\n","core/products/event":"<div class=\"form-field form-field-date form-required\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{product.event_date.name}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"EventDate[Yr]\" required>\n          <option disabled selected>{{lang 'forms.date.year'}}</option>\n          {{#if product.event_date.earliest_year '==' product.event_date.latest_year}}\n            <option value=\"{{product.event_date.earliest_year}}\">{{product.event_date.earliest_year}}</option>\n          {{else}}\n            {{#for product.event_date.earliest_year product.event_date.latest_year}}\n              <option value=\"{{$index}}\">{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"EventDate[Mth]\" required>\n          <option disabled selected>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\">{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"EventDate[Day]\" required>\n          <option disabled selected>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\">{{$index}}</option>\n          {{/for}}\n        </select>\n      </span>\n    </span>\n\n  </span>\n\n  {{#if product.event_date.date_start}}\n    {{#if product.event_date.date_end}}\n      <span class=\"product-event-bounds\">{{lang 'forms.date.bounds' start_date=product.event_date.date_start end_date=product.event_date.date_end}}</span>\n    {{/if}}\n  {{/if}}\n</div>\n","components/products/bulk-pricing":"{{#if product.bulk_discount_rates.length}}\n  <div class=\"bulk-pricing-block\">\n    <h1 class=\"product-details-heading\">{{lang 'product.bulk_pricing.title'}}</h1>\n    <ul>\n      {{#each product.bulk_discount_rates}}\n        <li>\n          {{lang 'product.bulk_pricing.range' min=min max=max}}\n          {{#if type '===' 'percent'}}\n            {{lang 'product.bulk_pricing.percent' discount=discount.formatted}}\n          {{/if}}\n          {{#if type '===' 'fixed'}}\n            {{lang 'product.bulk_pricing.fixed' discount=discount.formatted}}\n          {{/if}}\n          {{#if type '===' 'price'}}\n            {{lang 'product.bulk_pricing.price' discount=discount.formatted}}\n          {{/if}}\n        </li>\n      {{/each}}\n    </ul>\n  </div>\n{{/if}}\n","components/common/quantity":"{{!\n  Quantity Widget with Increment/Decrement controls.\n\n  @param product\n         cart.item or product object\n}}\n\n<div class=\"quantity-control\" data-quantity-control=\"{{id}}\">\n  <input\n    class=\"form-input\"\n    type=\"number\"\n    name=\"qty{{#if product_id}}-{{id}}{{else}}[]{{/if}}\"\n    pattern=\"[0-9]*\"\n    value=\"{{#if quantity}}{{quantity}}{{else}}{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\n    min=\"{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}\"\n    max=\"{{#if max_purchase_quantity}}{{max_purchase_quantity}}{{/if}}\"\n    data-quantity-control-input\n  >\n  <button class=\"button-control-action\" data-quantity-control-action=\"increment\">\n    &#43;\n  </button>\n  <button class=\"button-control-action\" data-quantity-control-action=\"decrement\">\n    &#8211;\n  </button>\n</div>\n","components/common/share-buttons":"{{#if share_buttons}}\n  <nav class=\"share-buttons-nav\">\n    <ul class=\"share-buttons\" data-share-buttons>\n\n      {{#each share_buttons.buttons}}\n        {{#if service '===' 'facebook_like'}}\n          <li class=\"social-link facebook-like\">\n            <div class=\"fb-like\"\n              data-href=\"{{../../url}}\"\n              data-layout=\"button_count\"\n              data-action=\"like\"\n              data-show-faces=\"false\">\n            </div>\n          </li>\n        {{/if}}\n      {{/each}}\n\n      {{#each share_buttons.buttons}}\n        {{#if service '===' 'facebook'}}\n          <li class=\"social-link facebook\">\n            <a target=\"_blank\" href=\"http://www.facebook.com/sharer.php?u={{../../url}}\" class=\"facebook\">\n              {{> svg/social/facebook}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'twitter'}}\n          <li class=\"social-link twitter\">\n            <a target=\"_blank\" href=\"https://twitter.com/share?url={{../../url}}&amp;text={{../../title}}\" class=\"twitter\">\n              {{> svg/social/twitter}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'google_plusone'}}\n          <li class=\"social-link google-plus\">\n            <a target=\"_blank\" href=\"https://plus.google.com/share?url={{../../url}}\" class=\"google\">\n              {{> svg/social/googleplus}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'linkedin'}}\n          <li class=\"social-link linkedin\">\n            <a target=\"_blank\" href=\"http://www.linkedin.com/shareArticle?url={{../../url}}&amp;title={{../../title}}\" class=\"linkedin\">\n              {{> svg/social/linkedin}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'email'}}\n          <li class=\"social-link mail\">\n            <a href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\" class=\"email\">\n              {{> svg/social/email}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'print'}}\n          <li class=\"social-link print\">\n            <a class=\"print\" href=\"javascript:;\" onclick=\"window.print()\">\n              {{> svg/social/print}}\n            </a>\n          </li>\n        {{/if}}\n\n        {{#if service '===' 'pinterest'}}\n          <li class=\"social-link pinterest\">\n            <a target=\"_blank\" href=\"//pinterest.com/pin/create/button/?url={{../../url}}&amp;media={{../../image}}&amp;description={{../../title}}\" class=\"share-pinterest\">\n              {{> svg/social/pinterest}}\n            </a>\n          </li>\n        {{/if}}\n      {{/each}}\n\n    </ul>\n  </nav>\n{{/if}}\n","core/forms/customizations/checkbox":"\n<div class=\"form-field form-field-customizations form-field-checkbox{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-control\">\n      <input class=\"form-input form-checkbox\"\n        id=\"product-field-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"checkbox\"\n        {{#if value}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text product-customization-text\">\n        {{display_name}}\n        {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/file":"\n<div class=\"form-field form-field-customizations form-field-file {{#if required}}form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/customizations/select":"\n<div class=\"form-field form-field-customizations form-field-select{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper\">\n        <select\n          class=\"form-input input-select\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\" {{#any values selected=true}}{{else}}selected{{/any}} disabled>{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{this}}\" {{#if selected}}selected{{/if}}>{{this}}</option>\n          {{/each}}\n        </select>\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/text":"\n<div class=\"form-field form-field-customizations form-field-text{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\" for=\"product-fields-{{id}}\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"{{value}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/customizations/textarea":"\n<div class=\"form-field form-field-customizations form-field-textarea{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}:\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        rows=\"4\"\n        placeholder=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"></textarea>\n    </span>\n  </label>\n</div>\n","core/forms/options/date":"<div class=\"form-field form-field-options form-field-date{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"date\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][year]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.year'}}</option>\n          {{#if earliest_year '===' latest_year}}\n            <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\n          {{else}}\n            {{#for earliest_year latest_year}}\n              <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][month]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][day]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>{{$index}}</option>\n          {{/for}}\n        </select>\n        </span>\n    </span>\n\n  </span>\n</div>\n","core/forms/options/input-checkbox":"\n<div class=\"form-field form-field-options form-field-checkbox{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-checkbox\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text\">{{label}}</span>\n    </label>\n  </div>\n</div>\n","core/forms/options/input-file":"\n<div class=\"form-field form-field-options form-field-file {{#if required}}form-required{{/if}}\"\n  data-product-attribute=\"input-file\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/options/input-numbers":"\n<div class=\"form-field form-field-options form-field-number{{#if required}} form-required{{/if}}\"\n data-product-attribute=\"input-number\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\" id=\"attribute-{{id}}\" name=\"attribute[{{id}}]\" type=\"number\" value=\"{{prefill}}\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/options/input-text":"\n<div class=\"form-field form-field-options form-field-text{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-text\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/options/product-list":"\n<div class=\"form-field form-field-options form-field-product-list{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"product-list\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#unless values.0.image}}\n      {{!-- same as radio buttons if no image set --}}\n      {{#each values}}\n        <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\"\n            value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"form-label-text\">{{label}}</span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"form-label-text\">{{lang 'product.none'}}</span>\n        </label>\n      {{/unless}}\n    {{else}}\n      {{#each values}}\n        <label class=\"form-label product-picklist-item\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\" value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"product-picklist-item-body\">\n            <span class=\"product-picklist-image-wrap\">\n              <img class=\"product-picklist-image\" src=\"{{getImage image 'core-product-picklist' (cdn ../theme_settings.default_image)}}\" alt=\"{{image.alt}}\">\n            </span>\n            <span class=\"product-picklist-label-text form-label-text\">{{label}}</span>\n          </span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label product-picklist-item\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}>\n          <span class=\"product-picklist-item-body picklist-none\">\n            <span class=\"product-picklist-label-text form-label-text\">{{lang 'product.none'}}</span>\n          </span>\n        </label>\n      {{/unless}}\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/set-radio":"\n<div class=\"form-field form-field-options form-field-radio{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-radio\">\n  <div class=\"form-field-title product-option-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-radio\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","core/forms/options/set-rectangle":"\n<div class=\"form-field form-field-options form-field-rectangle{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-rectangle\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label rectangle\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"rectangle-text form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n  </div>\n</div>\n","core/f